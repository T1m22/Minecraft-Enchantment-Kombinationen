    return total     

item = input("\nItem: ")

def total_enchantments():
    total = 0
    enchantments = enchants(item)
    for n in range(1, len(enchantments) + 1):
        if n == 1:
            print(f"\nE{n} = {value(enchantments1(n))}")
            total += enchantments1(n)
        if n == 2:
            print(f"E{n} = {value(enchantments2(n))}")
            total += enchantments2(n)
        if n == 3:
            print(f"E{n} = {value(enchantments3(n))}")
            total += enchantments3(n)
        if n == 4:
            print(f"E{n} = {value(enchantments4(n))}")
            total += enchantments4(n)
        if n == 5:
            print(f"E{n} = {value(enchantments5(n))}")
            total += enchantments5(n)
        if n == 6:
            print(f"E{n} = {value(enchantments6(n))}")
            total += enchantments6(n)
        if n == 7:
            print(f"E{n} = {value(enchantments7(n))}")
            total += enchantments7(n)
        if n == 8:
            print(f"E{n} = {value(enchantments8(n))}")
            total += enchantments8(n)
        if n == 9:
            print(f"E{n} = {value(enchantments9(n))}")
            total += enchantments9(n)
    print(f"\nGesamtanzahl der möglichen Enchantments: {value(total)}\n")

total_enchantments()
